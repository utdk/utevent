{
  "name": "utexas/utevent",
  "type": "drupal-custom-module",
  "description": "Drupal module that provides an event content type and supporting components for use with the UT Drupal Kit.",
  "keywords": [
    "Drupal"
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "homepage": "https://github.austin.utexas.edu/eis1-wcs/utevent",
  "minimum-stability": "dev",
  "support": {
    "issues": "https://github.austin.utexas.edu/eis1-wcs/utevent/issues",
    "source": "https://github.austin.utexas.edu/eis1-wcs/utevent"
  },
  "require": {
    "cweagans/composer-patches": "~1.0",
    "drupal/addtocal_augment": "1.0.0",
    "drupal/date_ap_style": "1.5.0",
    "drupal/fullcalendar_view": "5.1.8",
    "drupal/smart_date": "3.6.1",
    "utexas/utdk_profile": "^3.9.0 || dev-develop"
  },
  "extra": {
    "patches": {
      "drupal/date_ap_style": {
        "Show 'all day' field formatter setting not working correctly": "https://drupalkit.its.utexas.edu/patches/193-time-not-showing-utevent.patch"
      }
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "composer/installers": true,
      "oomphinc/composer-installers-extender": true
    }
  },
  "prefer-stable": true
}
