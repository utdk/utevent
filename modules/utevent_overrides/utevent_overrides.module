<?php

/**
 * @file
 * Utevent overrides.
 */

use Drupal\Core\Form\FormStateInterface;

use Drupal\utprof_overrides\Form\FieldFormAlterations;

/**
 * Implements hook_form_FORM_ID_alter().
 */
function utevent_overrides_form_field_config_edit_form_alter(&$form, FormStateInterface $form_state, $form_id) {
  // Modify field forms using Drupal\utevent_overrides\Form.
  \Drupal::service('class_resolver')
    ->getInstanceFromDefinition(FieldFormAlterations::class)
    ->fieldConfigEditFormAlter($form, $form_state, $form_id);
}
